{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@demo/*": ["src/*"],
      "@demo/components/*": ["src/components/*"],
      "@demo/lib/*": ["src/lib/*"],
      "@demo/layouts/*": ["src/layouts/*"],
      "@demo/pages/*": ["src/pages/*"],
      "@demo/styles/*": ["src/styles/*"],
      "@eqtylab/equality": ["../ui/src"],
      "@eqtylab/equality/*": ["../ui/src/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "skipLibCheck": true
  }
}
